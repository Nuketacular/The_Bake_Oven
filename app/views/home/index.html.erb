  <div class="page">
    <%= render 'header' %>
    <%= render 'homeSide' %>
      <section class="news">
	<h1 class="newsBanner"><a id="newsLink">News</a></h1>
	<div class="newsContent">
          <%= render partial: 'home_page_news', collection: @store_news, as: "store_news" %>
        </div>
      </section>
      <section class="deals">
	<h1 class="dealBanner"><a id="dealsLink">Deals</a></h1>
	<div class="dealContent">
           <%= render partial: 'home_page_deals', collection: @productDeals, as: "product" %>
        </div>
      </section>
      <section class="newItems">
        <h1 class="itemBanner"><a id="itemLink">New Items</a></h1>
	<div class="itemContent">
          <%= render partial: 'home_page_deals', collection: @productNew, as: "product" %>
        </div>
      </section>
      <footer class="footer">
        <%= render 'footer' %>
      </footer>
    </div>
